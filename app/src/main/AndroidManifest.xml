<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wedietapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.DarkActionBar">
        <activity
            android:name=".CrossTrainingActivity"
            android:exported="false" />
        <activity
            android:name=".LowerBodyBurnoutActivity"
            android:exported="false" />
        <activity
            android:name=".YogaActivity"
            android:exported="false" />
        <activity
            android:name=".WorkoutMenuActivity"
            android:exported="false" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".SetGoalActivity"
            android:exported="true"
            android:label="Set your weekly goal" />
        <activity
            android:name=".UserInfoActivity"
            android:exported="true"
            android:label="User information" /> <!-- <activity android:name=".MenuActivity" /> -->
        <activity android:name=".LogInActivity" />
        <activity
            android:name=".RegistrationActivity"
            android:label="Register Form"
            android:parentActivityName=".LogInActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>